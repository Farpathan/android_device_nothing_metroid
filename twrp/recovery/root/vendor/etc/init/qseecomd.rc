on init
    start vendor.qseecomd

service vendor.qseecomd /system/bin/sh /system/bin/qseecomd_wrapper.sh
    socket notify-topology stream 660 system drmrpc
    class core
    user root
    group root drmrpc
    seclabel u:r:recovery:s0

on property:sys.listeners.registered=true
    setprop vendor.sys.listeners.registered true

