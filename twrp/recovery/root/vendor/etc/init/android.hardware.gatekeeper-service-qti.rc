service vendor.gatekeeper-qti /system/bin/android.hardware.gatekeeper-service-qti
    interface aidl android.hardware.gatekeeper.IGatekeeper/default
    class early_hal
    user root
    group root
    setenv LD_LIBRARY_PATH /vendor/lib64:/vendor/lib:/system/lib64:/system/lib:/sbin
    setenv LD_PRELOAD /system/lib64/libcpp_compat.so
    disabled
    seclabel u:r:recovery:s0

on property:vendor.sys.listeners.registered=true
    start vendor.gatekeeper-qti
